{
    "extends": "strongloop",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "max-len": [
            1,
            110,
            4
        ],
        "strict": [
            2,
            "global"
        ]
    }
}